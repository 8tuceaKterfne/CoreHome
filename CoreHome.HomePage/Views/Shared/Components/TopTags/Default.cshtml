@model List<Tag>

<selection>
    <div class="card">
        <div class="card-header">Top Tags</div>
        <div class="card-body">
            <div class="toptags-list">

                @if (Model.Count == 0)
                {
                    <a class="btn btn-success">There's nothing here.</a>
                }
                else
                {
                    @for (var i = 0; i < Model.Count; i++)
                    {
                        <a class="btn btn-success">@Model[i].TagName</a>
                    }
                }

            </div>
        </div>
    </div>
</selection>