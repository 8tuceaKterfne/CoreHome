@inject HomePageService homePageService

<div class="container">

    <h1 class="display-4">Project</h1>
    <br />
    <a asp-controller="Project" asp-action="Add" class="btn btn-primary">Add Project</a>
    <br />
    <hr />

    <div id="projectList">

        <div class="row">
            @foreach (var project in homePageService.Config)
            {
                @await Component.InvokeAsync("Project", project)
            }
        </div>

    </div>
</div>